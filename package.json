{
  "name": "my-dream-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "eslint src --ext .ts",
    "lint-fix": "eslint src --fix --ext .ts",
    "nuke:cache": "rm -rf ./node_modules/.cache",
    "e2e": "ng e2e",
    "prepare": "husky install",
    "postinstall": "husky install"
